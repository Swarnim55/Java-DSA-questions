import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    boolean isPrime = isPrime(a);
    System.out.println(isPrime);
  }
  

   static boolean isPrime(int a) 
  {
    if(a<1)
      return false;
    int c = 2;
    while(c<a)
      {
        if(a%c==0)
          return false;
        else
          c = c + 1;
    }
    return true;
    }
}
