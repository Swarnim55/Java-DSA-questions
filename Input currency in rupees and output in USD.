import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    System.out.println("Enter currency in rupees");
    float x = in.nextFloat();
    float USD;
    USD = x/81.81f;
    
    
      System.out.println("The currency in USD is " +USD);
  }
}
