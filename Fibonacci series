import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int n = in.nextInt();
    int n1=0,n2=1,n3;
    System.out.print(n1 + "," + n2); //first print 0 and 1
    int i;
    for(i=1;i<=n;i++)
    {
        n3 = n1+n2;   
        System.out.print("," +n3);
        n1=n2;  //then swap
        n2=n3;
    }
  }
}
      
   
